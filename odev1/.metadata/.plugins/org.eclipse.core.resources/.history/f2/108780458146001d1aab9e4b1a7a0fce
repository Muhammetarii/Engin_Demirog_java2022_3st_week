package javaApp;

public class Customer {
	public Customer() {
		System.out.println("Müşteri nesnesi başlatıldı");
		
	}
	public int id;
	public String firstName;
	public String lastName;
	public String nationalIdentity;
	public String city;
	
	public class Company extends Customer{ // Inherit işlemi 
		
	}

}
