package javaApp;

public class Main {

	public static void main(String[] args) {
		
		int number1 = 10;
		int number2 = 20;
		number1 = number2;
		number2 = 100;
		// int değer tip olup sadece değer ataması yapar.Örnekte verilen number1 değeri 10 iken number2 nun sayı değeri olan 20 ye eşitlenir.
		
		int[] numbers1 = new int[1,2,3];
		int[] numbers2 = { 10, 20, 30 };
		numbers1 = numbers2 ;
		numbers2[0] = 1000;
		
		System.out.println(numbers1[0]);// Array lar değer tip değil referans tiptir.
		
	

	}

}
